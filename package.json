{
  "name": "license-webpack-plugin",
  "version": "2.0.0-alpha.3",
  "description": "Outputs licenses from 3rd party libraries to a file",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "MIGRATION.md",
    "DOCUMENTATION.md"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "prettier --single-quote --write 'src/**/*.ts' && tslint 'src/**/*.ts'",
    "build": "npm run lint && npm run clean && tsc",
    "prepublishOnly": "npm run build",
    "jest": "jest",
    "test": "npm run build && npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/xz64/license-webpack-plugin.git"
  },
  "keywords": [
    "license",
    "plugin",
    "webpack"
  ],
  "author": "S K (xz64)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xz64/license-webpack-plugin/issues"
  },
  "homepage": "https://github.com/xz64/license-webpack-plugin#readme",
  "peerDependencies": {
    "webpack-sources": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/node": "^8.10.0",
    "@types/webpack-sources": "^0.1.4",
    "jest": "^22.4.3",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^2.7.2"
  }
}
